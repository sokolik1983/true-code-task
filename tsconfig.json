{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true, // Убедитесь, что эта опция включена
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "src",
    "paths": {
      "*": ["*"]
    },
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "include": ["src", "custom.d.ts"]
}